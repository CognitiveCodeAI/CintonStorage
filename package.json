{
  "name": "cinton-storage",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "db:generate": "pnpm --filter @cinton/db generate",
    "db:migrate": "pnpm --filter @cinton/db migrate:dev",
    "db:seed": "pnpm --filter @cinton/db seed",
    "db:studio": "pnpm --filter @cinton/db studio"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0"
  }
}
